{
  "name": "ngx-jsonresume",
  "version": "0.0.1",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "build": "ng build",
    "test": "ng test",
    "test:travis": "ng run web:travis --configuration=test --watch=false --no-progress --browsers=ChromeHeadlessNoSandbox",
    "lint": "ng lint",
    "e2e": "ng e2e",
    "build:ssr": "ng run web:server && npm run webpack:ssr",
    "webpack:ssr": "webpack --config projects/server/webpack.server.config.js --colors",
    "serve:ssr": "node dist/web-webpack/server.js",
    "build:prod": "ng build web --prod && npm run build:ssr && npm run build:functions && npm run copy:hosting",
    "copy:hosting": "node ./tools/copy-dist.js",
    "build:functions": "cd functions; npm install; cd -; npm run --prefix functions build",
    "deploy": "npm run build:prod && firebase deploy --force",
    "ci": "npm run format:test && ng lint && npm run test:travis",
    "format:write": "prettier projects/**/*.{ts,md,scss} --write",
    "format:test": "prettier projects/**/*.{ts,md,scss} --list-different && ng lint",
    "analyze": "npm run build --prod --stats-json && webpack-bundle-analyzer ./dist/web/stats-es2015.json"
  },
  "husky": {
    "hooks": {
      "pre-commit": "pretty-quick --staged && ng lint --fix"
    }
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "~8.2.3",
    "@angular/cdk": "^8.1.4",
    "@angular/common": "~8.2.3",
    "@angular/compiler": "~8.2.3",
    "@angular/core": "~8.2.3",
    "@angular/fire": "^5.2.1",
    "@angular/flex-layout": "^8.0.0-beta.26",
    "@angular/forms": "^8.2.3",
    "@angular/material": "^8.1.4",
    "@angular/platform-browser": "~8.2.3",
    "@angular/platform-browser-dynamic": "~8.2.3",
    "@angular/platform-server": "^8.2.3",
    "@angular/pwa": "^0.803.0",
    "@angular/router": "~8.2.3",
    "@angular/service-worker": "~8.2.3",
    "@fortawesome/angular-fontawesome": "^0.4.0",
    "@fortawesome/fontawesome-free": "^5.8.2",
    "@fortawesome/fontawesome-svg-core": "^1.2.18",
    "@fortawesome/free-brands-svg-icons": "^5.8.2",
    "@fortawesome/free-solid-svg-icons": "^5.8.2",
    "@nguniversal/express-engine": "^8.1.1",
    "@nguniversal/module-map-ngfactory-loader": "^8.1.1",
    "@ngx-formly/core": "^5.4.1",
    "@ngx-formly/material": "^5.4.1",
    "@ngx-translate/core": "^11.0.1",
    "@ngx-translate/http-loader": "^4.0.0",
    "@terminus/ngx-tools": "^7.2.4",
    "@terminus/ui": "^14.11.0",
    "bootstrap": "^4.3.1",
    "firebase": "^6.6.1",
    "rxjs": "~6.4.0",
    "zone.js": "~0.9.1"
  },
  "devDependencies": {
    "@angular-builders/custom-webpack": "^8.2.0",
    "@angular-devkit/build-angular": "~0.803.0",
    "@angular-devkit/build-ng-packagr": "~0.803.0",
    "@angular/cli": "~8.3.0",
    "@angular/compiler-cli": "~8.2.3",
    "@angular/language-service": "~8.2.3",
    "@types/jasmine": "~3.3.8",
    "@types/jasminewd2": "~2.0.3",
    "@types/json-schema": "^7.0.3",
    "@types/node": "~8.9.4",
    "codelyzer": "^5.0.0",
    "dotenv-webpack": "^1.7.0",
    "express": "^4.17.1",
    "firebase-admin": "^8.4.0",
    "firebase-functions": "^3.2.0",
    "fs-extra": "^8.1.0",
    "husky": "^3.0.5",
    "jasmine-core": "~3.4.0",
    "jasmine-spec-reporter": "~4.2.1",
    "karma": "~6.3.14",
    "karma-chrome-launcher": "~2.2.0",
    "karma-coverage-istanbul-reporter": "~2.0.4",
    "karma-jasmine": "~2.0.1",
    "karma-jasmine-html-reporter": "^1.4.1",
    "karma-spec-reporter": "^0.0.32",
    "ng-packagr": "^5.4.0",
    "npm-run-all": "^4.1.5",
    "prettier": "^1.17.1",
    "pretty-quick": "^1.11.1",
    "protractor": "~5.4.0",
    "ts-loader": "^6.0.4",
    "ts-node": "~7.0.0",
    "tsickle": "^0.36.0",
    "tslib": "^1.10.0",
    "tslint": "~5.15.0",
    "tslint-angular": "^3.0.2",
    "tslint-config-prettier": "^1.18.0",
    "typescript": "~3.5.3",
    "webpack-bundle-analyzer": "^3.3.2",
    "webpack-cli": "^3.3.7"
  }
}
